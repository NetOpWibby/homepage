{
  "author": {
    "name": "netop://ウエハ",
    "url": "https://inc.sh"
  },
  "dependencies": {
    "dedent": "^0.7.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.73",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^3.0.0",
    "@webb/config-nyc": "*",
    "@webb/test": "*",
    "chronver": "*",
    "del-cli": "^4.0.1",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.50.0",
    "sass-lint": "^1.13.1",
    "stylelint": "^14.7.1",
    "stylelint-order": "^5.0.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "updates": "^13.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "name": "homepage",
  "private": true,
  "scripts": {
    "build": "npm run clean;npm run render;svelte-kit build",
    "clean": "del './build'",
    "increment": "chronver --increment package",
    "pre-commit": "npm run build && npm run increment && git add -A :/",
    "render": "sass sass/uchu:static --style compressed",
    "start": "npm i;npm run build;PORT=6433 SETTINGS=production node build/index.js",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:svelte": "svelte-check",
    "test:typescript": "tsc --noEmit",
    "watch": "npm run render && run-p watch:*",
    "watch:develop": "SETTINGS=development svelte-kit dev --port 6433 --open",
    "watch:sass": "sass --watch sass/uchu:static --style compressed"
  },
  "type": "module",
  "version": "2022.04.18"
}
