{
  "_moduleAliases": {
    "~component": "src/components",
    "~root": ".",
    "~util": "src/utils"
  },
  "author": {
    "name": "netop://ウエハ",
    "url": "https://webb.page"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "marked": "^1.0.0",
    "natural-orderby": "^2.0.3",
    "polka": "next",
    "sirv": "^0.4.2"
  },
  "description": "Homepage for netop:// ウエハ",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@inc/uchu": "^2019.12.1",
    "@webb/config-eslint": "^2020.4.16",
    "@webb/config-stylelint": "^2020.4.16",
    "chronver": "^2020.4.16",
    "dedent": "^0.7.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "link-module-alias": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.12",
    "sass": "^1.26.5",
    "snazzy": "^8.0.0",
    "standardx": "^5.0.0",
    "stylelint": "^13.3.3",
    "stylelint-order": "^4.0.0",
    "svelte": "^3.21.0",
    "svelte-preprocess": "^3.7.4",
    "updates": "^10.2.10"
  },
  "engines": {
    "node": ">=13.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run test:sass && npm run increment && git add -A :/"
    }
  },
  "name": "homepage",
  "private": true,
  "scripts": {
    "build": "sapper build --legacy",
    "compress": "sass --load-path=node_modules --update sass:static --style compressed",
    "export": "sapper export --legacy",
    "format": "eslint . --fix",
    "increment": "chronver --increment package",
    "postinstall": "link-module-alias",
    "preinstall": "command -v link-module-alias;link-module-alias clean || true",
    "start": "del './__sapper__';npm i;npm run build;npm run compress;NODE_ENV=production PORT=6433 node __sapper__/build",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:lint": "standardx --verbose | snazzy",
    "test:sass": "stylelint 'sass/*/**.scss'",
    "watch": "NODE_ENV=development PORT=6433 run-p watch:*",
    "watch:app": "sapper dev",
    "watch:sass": "sass --load-path=node_modules --watch sass:static --style compressed"
  },
  "standardx": {
    "ignore": [
      "__sapper__",
      "static"
    ]
  },
  "version": "2020.04.26"
}
